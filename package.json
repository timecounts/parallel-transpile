{
  "name": "parallel-transpile",
  "version": "0.5.0",
  "description": "Transpile files in parallel using all available CPUs",
  "main": "index.js",
  "bin": {
    "parallel-transpile": "./bin/parallel-transpile"
  },
  "scripts": {
    "test": "mocha test/**/*-test.*",
    "test:watch": "mocha -w test/**/*-test.*",
    "prepublish": "coffee -cbmo . src",
    "watch": "coffee -cwbmo . src"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:timecounts/parallel-transpile.git"
  },
  "keywords": [
    "transpile",
    "compile",
    "build",
    "parallel",
    "concurrent",
    "multicore",
    "processors",
    "coffee",
    "coffee-script",
    "cjsx",
    "stylus",
    "less",
    "jade"
  ],
  "author": "Benjie Gillam <benjie@jemjie.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/timecounts/parallel-transpile/issues"
  },
  "homepage": "https://github.com/timecounts/parallel-transpile",
  "dependencies": {
    "debug": "^2.2.0",
    "enhanced-resolve": "^0.9.1",
    "minimist": "^1.1.1",
    "mkdirp": "^0.5.0",
    "source-map": "^0.5.3"
  },
  "devDependencies": {
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-es2015-native-modules": "6.5.0",
    "babel-preset-react": "6.5.0",
    "chai": "3.5.0",
    "chokidar": "^1.0.1",
    "cjsx-loader": "^2.0.1",
    "coffee-loader": "^0.7.2",
    "coffee-script": "^1.10.0",
    "jsx-loader": "^0.13.1",
